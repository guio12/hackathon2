{% extends 'base.html.twig' %}

{% block body %}
    <h1>Maptype</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ mapType.id }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ mapType.type }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('maptype_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('maptype_edit', { 'id': mapType.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
